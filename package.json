{
  "name": "personal-asset-portfolio",
  "version": "2.0.0",
  "description": "个人投资组合管理系统 - 基于Cloudflare Workers和D1数据库",
  "main": "src/worker.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:rebuild": "wrangler d1 execute personasset --file=d1-schema-no-fk.sql",
    "db:seed": "wrangler d1 execute personasset --file=test-data.sql",
    "db:rebuild:remote": "wrangler d1 execute personasset --remote --file=d1-schema-no-fk.sql",
    "db:seed:remote": "wrangler d1 execute personasset --remote --file=test-data.sql"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "itty-router": "^4.0.0"
  },
  "devDependencies": {
    "wrangler": "^4.28.1"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "d1",
    "sqlite",
    "asset-management",
    "portfolio",
    "serverless"
  ],
  "author": "Cyberteng",
  "license": "GPL-3.0"
}
